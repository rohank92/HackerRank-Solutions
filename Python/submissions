#challenges/py-if-else

if __name__ == '__main__':
    n = int(input().strip())
    
    if ( n % 2 == 1 or (n%2==0 and n>=6 and n<=20)):
        print("Weird")
    elif ( n%2==0 and n>=2 and n<=5):
        print("Not Weird")
    elif ( n> 20 and n%2==0):
        print("Not Weird")

